{
	"extends": "@rocketseat/eslint-config/node",
  "rules": {
    "no-useless-constructor": "off",
    "no-new": "off",
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto",
        "singleQuote": true,
        "trailingComma": "all",
        "semi": true,
        "bracketSpacing": true
      }
    ]
  }
}